<% @tags.each do |tag| %>
  <li data-autocomplete-target="resultItem" class="list-group-item w-80 flex items-center gap-x-3.5 py-2 px-3 text-sm text-gray-800 bg-white hover:bg-blue-200 focus:ring-2 focus:ring-blue-500 dark:text-gray-400 dark:hover:bg-blue-200 dark:hover:text-gray-300" role="option" data-autocomplete-value="<%= tag.id %>" data-autocomplete-label="<%= tag.name %>">
    <%= tag.name %>
  </li>
<% end %>